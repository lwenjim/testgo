{
    "languageserver": {
        "golang": {
            "command": "/Users/jim/Workdata/goland/bin/gopls",
            "rootPatterns": ["go.mod", "go.work", ".git"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true,
                "analyses": {
                    "unreachable": false,
                    "unusedvariable": true
                }
            },
            "args": ["-remote", "127.0.0.1:39999"]
        },
        "clangd": {
            "command": "clangd",
            "rootPatterns": ["compile_flags.txt", "compile_commands.json"],
            "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"]
        },
        "bash": {
            "command": "bash-language-server",
            "args": ["start"],
            "filetypes": ["sh"]
        },
        "lua": {
            "command": "lua-language-server",
            "args": ["-E", "/Users/jim/.vscode/extensions/sumneko.lua-3.10.6-darwin-x64/server/main.lua"],
            "rootPatterns": [".git"],
            "filetypes": ["lua"]
        }
    },
    "diagnostic.refreshOnInsertMode":true,
    "inlayHint.enable": false,
    "inlayHint.enableParameter": false,
    "java.jdt.ls.java.home":"/Users/jim/Library/Java/JavaVirtualMachines/openjdk-22.0.1/Contents/Home",
    "java.jdt.ls.vmargs": "-Xmx1G",
    "java.configuration.maven.userSettings": "",
    "java.format.enabled": true,
    "java.completion.guessMethodArguments": true,
    "java.saveActions.organizeImports": true,
    "java.referencesCodeLens.enabled": true,
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-20",
            "path": "/Users/jim/Library/Java/JavaVirtualMachines/openjdk-22.0.1/Contents/Home",
            "default": true
        }
    ],
    "[java]": {
